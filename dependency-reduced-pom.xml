<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.github.kamir</groupId>
  <artifactId>rosbag-extractor</artifactId>
  <version>0.1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <excludes>
                  <exclude>junit:junit</exclude>
                </excludes>
              </artifactSet>
              <transformers>
                <transformer>
                  <resource>META-INF/cxf/bus-extensions.txt</resource>
                </transformer>
              </transformers>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                  </excludes>
                </filter>
              </filters>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>cdh.repo</id>
      <name>Cloudera Repositories</name>
      <url>https://repository.cloudera.com/artifactory/cloudera-repos</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots />
      <id>cdh.snapshots.repo</id>
      <name>Cloudera Snapshots Repository</name>
      <url>https://repository.cloudera.com/artifactory/libs-snapshot-local</url>
    </repository>
  </repositories>
  <properties>
    <javafx.version>2.2.3</javafx.version>
    <datavec.version>0.9.1</datavec.version>
    <aws.sdk.version>1.11.109</aws.sdk.version>
    <nd4j.backend>nd4j-native-platform</nd4j.backend>
    <nd4j.version>0.9.1</nd4j.version>
    <maven-compiler-plugin.version>3.6.1</maven-compiler-plugin.version>
    <rl4j.version>0.9.1</rl4j.version>
    <exec-maven-plugin.version>1.4.0</exec-maven-plugin.version>
    <maven.minimum.version>3.3.1</maven.minimum.version>
    <scala.binary.version>2.11</scala.binary.version>
    <jfreechart.version>1.0.13</jfreechart.version>
    <arbiter.version>0.9.1</arbiter.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <hbase.version>1.2.0-cdh5.11.1</hbase.version>
    <dl4j.version>0.9.1</dl4j.version>
    <datavec.spark.version>0.9.1_spark_1</datavec.spark.version>
    <dl4j.spark.version>0.9.1_spark_1</dl4j.spark.version>
    <dcm4che.version>5.13.0</dcm4che.version>
    <java.version>1.8</java.version>
    <logback.version>1.1.7</logback.version>
    <maven-shade-plugin.version>2.4.3</maven-shade-plugin.version>
    <scala.plugin.version>3.2.2</scala.plugin.version>
    <jcommon.version>1.0.23</jcommon.version>
    <guava.version>19.0</guava.version>
    <jackson.version>2.6.6</jackson.version>
    <shadedClassifier>bin</shadedClassifier>
  </properties>
</project>

